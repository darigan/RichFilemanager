  "devDependencies": {
    "@types/clipboard": "^1.5.35",
    "@types/codemirror": "0.0.50",
    "@types/highlight.js": "^9.1.10",
    "@types/jquery": "^3.2.15",
    "@types/jquery-mousewheel": "^3.1.8",
    "@types/jquery.contextmenu": "^1.7.33",
    "@types/jquery.fileupload": "^5.40.36",
    "@types/jqueryui": "^1.11.37",
    "@types/knockout": "^3.4.46",
    "@types/purl": "^2.3.30",
