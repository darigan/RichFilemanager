{
  "name": "rich-filemanager",
  "version": "2.6.1",
  "description": "Highly customizable open-source file manager",
  "main": "scripts/filemanager.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/servocoder/RichFilemanager.git"
  },
  "keywords": [
    "filemanager",
    "fileupload",
    "manager",
    "upload",
    "file",
    "rich",
    "js",
    "php"
  ],
  "author": "Pavel Solomienko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/servocoder/RichFilemanager/issues"
  },
  "homepage": "https://github.com/servocoder/RichFilemanager#readme",
  "devDependencies": {
    "@types/clipboard": "^1.5.35",
    "@types/codemirror": "0.0.50",
    "@types/highlight.js": "^9.1.10",
    "@types/jquery": "^3.2.15",
    "@types/jquery-mousewheel": "^3.1.8",
    "@types/jquery.contextmenu": "^1.7.33",
    "@types/jquery.fileupload": "^5.40.36",
    "@types/jqueryui": "^1.11.37",
    "@types/knockout": "^3.4.46",
    "@types/purl": "^2.3.30",
    "awesome-typescript-loader": "^3.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "ejs-loader": "^0.3.0",
    "eslint": "^4.10.0",
    "expose-loader": "^0.7.3",
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-uglify": "^1.0.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-gyp": "^3.6.2",
    "npm-run-all": "^4.0.2",
    "purl": "^1.0.4",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "tslib": "^1.8.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "uglify-js": "^3.0.27",
    "webpack": "^3.8.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4"
  },
  "scripts": {
    "uglify:1": "uglifyjs ./scripts/filemanager.js -c -m -o ./scripts/filemanager.min.js",
    "uglify:2": "uglifyjs ./scripts/jquery-ui/jquery-ui.js -c -m -o ./scripts/jquery-ui/jquery-ui.min.js",
    "uglify:3": "uglifyjs ./scripts/jquery.fileDownload/jquery.fileDownload.js -c -m -o ./scripts/jquery.fileDownload/jquery.fileDownload.min.js",
    "uglify:4": "uglifyjs ./scripts/lazyload/dist/lazyload.js -c -m -o ./scripts/lazyload/dist/lazyload.min.js",
    "uglify:5": "uglifyjs ./scripts/purl/purl.js -c -m -o ./scripts/purl/purl.min.js",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --inline --progress --watch --open",
    "server:prod": "cross-env NODE_ENV=production webpack-dashboard -- webpack-dev-server --config ./webpack.config.js --port 3000 --host 0.0.0.0 --hot --inline --progress --profile --watch --open --content-base dist/",
    "build": "npm run build:dev",
    "build:dev": "webpack --config ./webpack.config.js --progress --profile --color --display-error-details --display-cached",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js  --progress --profile --color --display-error-details --display-cached --bail",
    "clean": "npm cache clear && rimraf -- dist"
  },
  "dependencies": {
    "vanilla-lazyload": "^8.2.1"
  }
}
